<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #2e2e2e;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HomePage">
   <children>
      <ButtonBar layoutX="57.0" layoutY="28.0" prefHeight="34.0" prefWidth="280.0">
        <buttons>
          <Button fx:id="FilmButton" mnemonicParsing="false" style="-fx-background-color: #2e2e2e;" text="Film" textFill="WHITE">
