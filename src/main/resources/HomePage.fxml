         <image>
            <Image url="@Images/default-avatar.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="83.0" fitWidth="113.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/download.png" />
         </image>
      </ImageView>
      <Button fx:id="AccountButton" layoutX="744.0" layoutY="24.0" mnemonicParsing="false" prefHeight="41.0" prefWidth="42.0" style="-fx-opacity: 0;" text="Account" textFill="TRANSPARENT" />
